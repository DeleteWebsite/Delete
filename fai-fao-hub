                                      until v.Humanoid.Health <= 0 or not v.Parent or _G.AutoRaidPirate == false
                                      Fastattack = false
                                      MakoriGayMag = false
                                  end
                              end
                          end
                      else
                          if ((CFrameBoss).Position - game.Players.LocalPlayer.Character.HumanoidRootPart.Position).magnitude <= 1500 then
                              topos(CFrameBoss)
                          else
                              TelePBoss(CFrameBoss)
                          end
                      end
                  end)
              end
          end
